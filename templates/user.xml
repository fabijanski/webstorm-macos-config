<templateSet group="user">
  <template name=":tp" value="${props =&gt; props.theme.$END$}" description="Theme Props Snippet" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_PROPERTY_VALUE" value="true" />
    </context>
  </template>
  <template name=":cl" value="console.log('$END$')" description="console.log('')" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name=":cs" value="import css from 'styled-jsx/css'&#10;import styleVariables from '../../../globals/style-variables'&#10;&#10;/* language=SCSS */&#10;const styles = css`&#10;  $END$&#10;`&#10;&#10;export default styles" description="Custom Styles Template" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>